{
  "name": "fisioflow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Sistema completo de gestão de clínica de fisioterapia",
  "scripts": {
    "frontend:dev": "vite",
    "backend:dev": "nodemon",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "frontend:build": "tsc -b && vite build",
    "backend:build": "tsc --project tsconfig.json --outDir dist",
    "build": "npm run frontend:build",
    "test:unit": "vitest",
    "test:integration": "playwright test",
    "test": "npm run test:unit && npm run test:integration",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "db:seed": "tsx scripts/seed.ts",
    "db:backup": "tsx scripts/backup.ts",
    "deploy:production": "tsx scripts/deploy.ts"
  },
  "dependencies": {
